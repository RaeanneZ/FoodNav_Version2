<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".AccountPage">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="20dp">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:fontFamily="@font/inter_extrabold"
                android:text="My Account"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="38sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginStart="48dp"
                android:layout_marginTop="48dp"
                android:layout_marginEnd="11dp"
                app:layout_constraintEnd_toStartOf="@+id/tempName"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:srcCompat="@drawable/baseline_circle_24" />

            <TextView
                android:id="@+id/tempName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:fontFamily="@font/inter_semibold"
                android:text="tempName"
                android:textColor="@color/black"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView7"
                app:layout_constraintEnd_toEndOf="@+id/textView3"
                app:layout_constraintHorizontal_bias="0.378"
                app:layout_constraintStart_toStartOf="@+id/textView3"
                app:layout_constraintTop_toTopOf="@+id/imageView7"
                app:layout_constraintVertical_bias="0.512" />

            <View
                android:id="@+id/divider"
                style="@style/Divider.Horizontal.Lavender"
                android:layout_width="0dp"
                android:layout_marginHorizontal="30dp"
                android:layout_marginTop="28dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.413"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tempName" />

            <TextView
                android:id="@+id/planName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/inter_medium"
                android:text="Diabetic Friendly Plan"
                android:textColor="@color/greyblack"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="@+id/divider"
                app:layout_constraintTop_toBottomOf="@+id/divider" />

            <TextView
                android:id="@+id/settings"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="36dp"
                android:fontFamily="@font/inter_semibold"
                android:text="Settings"
                android:textColor="@color/black"
                android:textSize="28sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/planName" />

            <ImageView
                android:id="@+id/personIcon"
                android:layout_width="42dp"
                android:layout_height="38dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="24dp"
                app:layout_constraintEnd_toEndOf="@+id/settings"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/settings"
                app:layout_constraintTop_toBottomOf="@+id/settings"
                app:srcCompat="@drawable/baseline_person_24" />

            <TextView
                android:id="@+id/editProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/inter_regular"
                android:text="Edit profile"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/personIcon"
                app:layout_constraintStart_toEndOf="@+id/personIcon"
                app:layout_constraintTop_toTopOf="@+id/personIcon" />

            <ImageView
                android:id="@+id/arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="@+id/editProfile"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/editProfile"
                app:srcCompat="@drawable/baseline_chevron_right_24" />

            <View
                android:id="@+id/divider_lavender"
                style="@style/Divider.Horizontal.Lavender"
                android:layout_width="0dp"
                android:layout_marginTop="24dp"
                app:layout_constraintEnd_toEndOf="@+id/arrow"
                app:layout_constraintHorizontal_bias="0.44"
                app:layout_constraintStart_toStartOf="@id/settings"
                app:layout_constraintTop_toBottomOf="@+id/editProfile" />


            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/baseline_food_bank_24"
                app:layout_constraintEnd_toEndOf="@+id/settings"
                app:layout_constraintHorizontal_bias="0.022"
                app:layout_constraintStart_toStartOf="@+id/settings"
                app:layout_constraintTop_toBottomOf="@+id/divider_lavender" />

            <TextView
                android:id="@+id/dietPref"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/inter_regular"
                android:text="Dietary Profile"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView11"
                app:layout_constraintStart_toEndOf="@+id/imageView11"
                app:layout_constraintTop_toTopOf="@+id/imageView11" />

            <ImageView
                android:id="@+id/arrowDiet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="20dp"
                android:src="@drawable/baseline_chevron_right_24"
                app:layout_constraintBottom_toBottomOf="@+id/dietPref"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/dietPref" />

            <View
                android:id="@+id/divider_lavender_3"
                style="@style/Divider.Horizontal.Lavender"
                android:layout_width="0dp"
                android:layout_marginTop="24dp"
                app:layout_constraintEnd_toEndOf="@+id/divider_lavender"
                app:layout_constraintHorizontal_bias="0.44"
                app:layout_constraintStart_toStartOf="@+id/divider_lavender"
                app:layout_constraintTop_toBottomOf="@+id/dietPref" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/baseline_key_24"
                app:layout_constraintEnd_toEndOf="@+id/settings"
                app:layout_constraintHorizontal_bias="0.022"
                app:layout_constraintStart_toStartOf="@+id/settings"
                app:layout_constraintTop_toBottomOf="@+id/divider_lavender_3" />

            <TextView
                android:id="@+id/changePassword"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/inter_regular"
                android:text="Change password"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView8"
                app:layout_constraintStart_toEndOf="@+id/imageView8"
                app:layout_constraintTop_toTopOf="@+id/imageView8" />

            <View
                android:id="@+id/divider_lavender_2"
                style="@style/Divider.Horizontal.Lavender"
                android:layout_width="0dp"
                android:layout_marginTop="24dp"
                app:layout_constraintEnd_toEndOf="@+id/divider_lavender"
                app:layout_constraintHorizontal_bias="0.44"
                app:layout_constraintStart_toStartOf="@+id/divider_lavender"
                app:layout_constraintTop_toBottomOf="@+id/changePassword" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_marginRight="20dp"
                android:src="@drawable/baseline_chevron_right_24"
                app:layout_constraintBottom_toBottomOf="@+id/changePassword"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/arrow"
                app:layout_constraintTop_toTopOf="@+id/changePassword" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/inter_regular"
                android:text="Dark mode"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/imageView12"
                app:layout_constraintStart_toEndOf="@+id/imageView12"
                app:layout_constraintTop_toTopOf="@+id/imageView12"
                tools:visibility="gone" />

            <View
                android:id="@+id/divider_lavender_4"
                style="@style/Divider.Horizontal.Lavender"
                android:layout_width="0dp"
                android:layout_marginTop="24dp"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="@id/divider_lavender_2"
                app:layout_constraintStart_toStartOf="@id/divider_lavender_2"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                tools:visibility="gone" />

            <Button
                android:id="@+id/logoutButton"
                android:layout_width="315dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="98dp"
                android:layout_marginBottom="10dp"
                android:background="@color/lavender"
                android:fontFamily="@font/inter_semibold"
                android:text="Log Out"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/textView3"
                app:layout_constraintStart_toStartOf="@+id/textView3"
                app:layout_constraintTop_toBottomOf="@id/divider_lavender_4" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/baseline_dark_mode_24"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="@+id/settings"
                app:layout_constraintHorizontal_bias="0.131"
                app:layout_constraintStart_toStartOf="@+id/settings"
                app:layout_constraintTop_toBottomOf="@id/divider_lavender_2"
                tools:visibility="gone" />

            <Switch
                android:id="@+id/darkmode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/imageView12"
                app:layout_constraintEnd_toEndOf="@+id/imageView9"
                app:layout_constraintTop_toBottomOf="@id/divider_lavender_2"
                app:layout_constraintTop_toTopOf="@+id/imageView12"
                tools:visibility="gone" />

            <ImageView
                android:id="@+id/male_icon"
                android:layout_width="45dp"
                android:layout_height="54dp"
                android:src="@drawable/male"
                app:layout_constraintBottom_toBottomOf="@+id/imageView7"
                app:layout_constraintEnd_toEndOf="@+id/imageView7"
                app:layout_constraintStart_toStartOf="@+id/imageView7"
                app:layout_constraintTop_toTopOf="@+id/imageView7" />

            <ImageView
                android:id="@+id/female_icon"
                android:layout_width="45dp"
                android:layout_height="54dp"
                android:src="@drawable/female"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/imageView7"
                app:layout_constraintEnd_toEndOf="@+id/imageView7"
                app:layout_constraintStart_toStartOf="@+id/imageView7"
                app:layout_constraintTop_toTopOf="@+id/imageView7" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</FrameLayout>